<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<description>Example configuration to get you started.</description>

	<context:component-scan base-package="net.phreebie.spring" />

	<bean id="injectSimple" class="net.phreebie.spring.InjectSimple">
		<property name="name">
			<value>Ilya Juhnowski</value>
		</property>
		<property name="age">
			<value>37</value>
		</property>
		<property name="height">
			<value>176</value>
		</property>
		<property name="programmer">
			<value>true</value>
		</property>
		<property name="ageInSeconds">
			<value>1103760000</value>
		</property>
	</bean>
</beans>
